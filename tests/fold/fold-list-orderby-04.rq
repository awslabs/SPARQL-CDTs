PREFIX cdt:    <http://example.org/cdt/>

ASK {
	{
		SELECT (FOLD(?v ORDER BY ASC(?sort)) AS ?list)
		WHERE {
			VALUES (?sort ?v) {
				(1 1)
				(2 UNDEF)
				(3 3)
			}
		}
	}
	FILTER(cdt:get(?list, 1) = 1)
	FILTER(cdt:get(?list, 3) = 3)
	BIND(cdt:get(?list, 2) AS ?null)
	FILTER(!BOUND(?null))
}
