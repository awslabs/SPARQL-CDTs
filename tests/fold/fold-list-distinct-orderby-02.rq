PREFIX cdt:    <http://example.org/cdt/>

ASK {
	{
		SELECT (FOLD(DISTINCT ?v ORDER BY ASC(?sort)) AS ?list)
		WHERE {
			VALUES (?sort ?v ?other) {
				(1 1 "same")
				(1 1 "same")

				(3 3 "same")
				(3 3 "same")
				(3 3 "different")

				(2 2 "different a")
				(2 2 "different b")
			}
		}
	}

	FILTER(?list = "[1,2,2,3,3]"^^cdt:List)
}
