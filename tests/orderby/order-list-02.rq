PREFIX cdt:    <http://example.org/cdt/>
PREFIX xsd:    <http://www.w3.org/2001/XMLSchema#>

ASK {
	{
		SELECT * WHERE {
			VALUES (?id ?list) {
				(1  "ill-formed b"^^cdt:List)
				(2  "ill-formed a"^^cdt:List)
				(3  "ill-formed c"^^cdt:List)
			}
		}
		ORDER BY ?list LIMIT 1
	}
	FILTER (?id = 2)
}
